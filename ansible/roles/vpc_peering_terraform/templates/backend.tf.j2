# -----------------------------------------------------------------------------
# S3 Backend Configuration for VPC Peering Terraform State
# Generated by Ansible - Do not edit manually
# -----------------------------------------------------------------------------

terraform {
  backend "s3" {
    bucket         = "{{ terraform_state_bucket }}"
    key            = "{{ terraform_state_key }}"
    region         = "{{ prod_cluster.region }}"
    encrypt        = true
    dynamodb_table = "{{ terraform_state_dynamodb_table }}"
  }
}
